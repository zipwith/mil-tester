-- not recursive
entrypoint mdirect :: [Bit 32, Bit 32, Unit] >>= [Bit 32]
mdirect[t0, t1, t2] =
  t3 <- lshr((t0, 30))
  t4 <- shl((t1, 2))
  t5 <- or((t3, t4))
  and((t5, 255))

