-- not recursive
b5 :: [] >>= [Word]
b5[] =
  return 2

-----------------------------------------
-- not recursive
b4 :: [] >>= [Word]
b4[] =
  return 1

-----------------------------------------
-- not recursive
b6 :: [BOOL] >>= [Word]
b6[t0] =
  case t0 of
    Tt -> b4[]
    Ff -> b5[]

