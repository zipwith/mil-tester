bitdata PCI /16
  = PCI [ bus :: Bit 8 | dev :: Bit 5 | fun :: Bit 3 ]
    -- predPCI(x :: Bit 16) = true

-----------------------------------------
-- not recursive
b38 :: [Bit 3, Bit 5, Bit 8] >>= [PCI]
b38[t0, t1, t2] =
  t3 <- PCI.PCI(t2, t1, t0)
  PCI(t3)

-----------------------------------------
-- not recursive
k12 :: {Bit 8, Bit 5} [Bit 3] ->> [PCI]
k12{t0, t1} t2 = b38[t2, t1, t0]

