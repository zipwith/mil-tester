-- not recursive
construct_T :: [Word, Word] >>= [Word]
construct_T[t0, t1] =
  t2 <- return 0
  t3 <- or((t2, t1))
  t4 <- shl((t0, 2))
  t5 <- or((t3, t4))
  return t5

-----------------------------------------
-- not recursive
s3 :: Word
s3 <-
  construct_T[1, 0]

-----------------------------------------
-- not recursive
entrypoint sample :: Word
sample <-
  return s3

-----------------------------------------
-- not recursive
s2 :: Word
s2 <-
  construct_T[1, 1]

-----------------------------------------
-- not recursive
entrypoint sample1 :: Word
sample1 <-
  return s2

-----------------------------------------
-- not recursive
s1 :: Word
s1 <-
  construct_T[15, 0]

-----------------------------------------
-- not recursive
entrypoint sample2 :: Word
sample2 <-
  return s1

