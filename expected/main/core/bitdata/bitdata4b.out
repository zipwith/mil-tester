-- not recursive
entrypoint rotate :: [Word] >>= [Word]
rotate[t0] =
  t1 <- and((t0, 2))
  t2 <- primNeq((t1, 0))
  t3 <- and((t0, 1))
  t4 <- primNeq((t3, 0))
  t5 <- and((t0, 4))
  t6 <- primNeq((t5, 0))
  t7 <- flagToWord((t6))
  t8 <- flagToWord((t4))
  t9 <- shl((t8, 1))
  t10 <- or((t7, t9))
  t11 <- flagToWord((t2))
  t12 <- shl((t11, 2))
  or((t10, t12))

