data Rainbow
  = Red
  | Orange
  | Yellow
  | Green
  | Blue
  | Indigo
  | Violet

data Unit
  = Unit

-----------------------------------------
-- not recursive
external print :: [Bit 32] ->> [[] ->> [Unit]]

-----------------------------------------
-- not recursive
b11 :: [] >>= [Unit]
b11[] =
  t0 <- print @ 1
  t0 @ []

-----------------------------------------
-- not recursive
b10 :: [] >>= [Unit]
b10[] =
  t0 <- print @ 3
  t0 @ []

-----------------------------------------
-- not recursive
b7 :: [] >>= [Unit]
b7[] =
  t0 <- print @ 2
  t0 @ []

-----------------------------------------
-- not recursive
b12 :: [Rainbow] >>= [Unit]
b12[t0] =
  case t0 of
    Red -> b11[]
    Blue -> b10[]
    _ -> b7[]

-----------------------------------------
-- not recursive
k3 :: {Rainbow} [] ->> [Unit]
k3{t0} [] = b12[t0]

-----------------------------------------
-- not recursive
k4 :: {} [Rainbow] ->> [[] ->> [Unit]]
k4{} t0 = k3{t0}

-----------------------------------------
-- not recursive
entrypoint test1 :: [Rainbow] ->> [[] ->> [Unit]]
test1 <-
  k4{}

-----------------------------------------
-- Entrypoints: test1
