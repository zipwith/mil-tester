data Rainbow
  = Red
  | Orange
  | Yellow
  | Green
  | Blue
  | Indigo
  | Violet

data Unit
  = Unit

-----------------------------------------
-- not recursive
external print :: [Bit 32] ->> [[] ->> [Unit]]

-----------------------------------------
-- not recursive
b8 :: [] >>= [Unit]
b8[] =
  t0 <- print @ 1
  t0 @ []

-----------------------------------------
-- not recursive
b5 :: [] >>= [Unit]
b5[] =
  t0 <- print @ 2
  t0 @ []

-----------------------------------------
-- not recursive
b9 :: [Rainbow] >>= [Unit]
b9[t0] =
  case t0 of
    Red -> b8[]
    Green -> b8[]
    Orange -> b8[]
    _ -> b5[]

-----------------------------------------
-- not recursive
k3 :: {Rainbow} [] ->> [Unit]
k3{t0} [] = b9[t0]

-----------------------------------------
-- not recursive
k4 :: {} [Rainbow] ->> [[] ->> [Unit]]
k4{} t0 = k3{t0}

-----------------------------------------
-- not recursive
entrypoint test1 :: [Rainbow] ->> [[] ->> [Unit]]
test1 <-
  k4{}

-----------------------------------------
-- Entrypoints: test1
