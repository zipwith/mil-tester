data Rainbow
  = Red
  | Orange
  | Yellow
  | Green
  | Blue
  | Indigo
  | Violet

-----------------------------------------
-- not recursive
b9 :: [] >>= [Bit 32]
b9[] =
  return 1

-----------------------------------------
-- not recursive
b7 :: [] >>= [Bit 32]
b7[] =
  return 2

-----------------------------------------
-- not recursive
b10 :: [Rainbow] >>= [Bit 32]
b10[t0] =
  case t0 of
    Red -> b9[]
    _ -> b7[]

-----------------------------------------
-- not recursive
k3 :: {} [Rainbow] ->> [Bit 32]
k3{} t0 = b10[t0]

-----------------------------------------
-- not recursive
entrypoint test1 :: [Rainbow] ->> [Bit 32]
test1 <-
  k3{}

-----------------------------------------
