-- not recursive
decodeSuccess :: [Word, Word, Word, Word] >>= [DecodeResult0]
decodeSuccess[t0, t1, t2, t3] =
  t4 <- shl((t2, t1))
  t5 <- sub((32, t1))
  t6 <- lshr((t2, t5))
  t7 <- shl((t6, 4))
  t8 <- and((t0, -32))
  t9 <- add((t8, t7))
  Success1(t9, t4, t3)

