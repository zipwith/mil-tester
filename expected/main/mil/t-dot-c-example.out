-- not recursive
entrypoint decodeCNode :: [Word, Word, Word] >>= [DecodeResult0]
decodeCNode[t0, t1, t2] =
  t3 <- and((t0, 31))
  t4 <- sub((t2, t3))
  t5 <- primSge((t4, 0))
  if t5
    then decodeSuccess[t0, t3, t1, t4]
    else decodeFailed[t2]

