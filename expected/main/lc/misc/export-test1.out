data List1
  = Nil2
  | Cons3 Word List1

data Unit
  = Unit

data ListWord0
  = LNil0
  | LCons1 Word ListWord0

-----------------------------------------
-- not recursive
entrypoint LNil :: ListWord0
LNil <-
  LNil0()

-----------------------------------------
-- not recursive
entrypoint LCons :: [Word, ListWord0] >>= [ListWord0]
LCons[t0, t1] =
  LCons1(t0, t1)

-----------------------------------------
-- not recursive
b67 :: [] >>= [Word]
b67[] =
  return 0

-----------------------------------------
-- recursive
b74 :: [List1] >>= [Word]
b74[t0] =
  assert t0 Cons3
  t1 <- Cons3 0 t0
  t2 <- Cons3 1 t0
  t3 <- sum[t2]
  add((t1, t3))
entrypoint sum :: [List1] >>= [Word]
sum[t0] =
  case t0 of
    Nil2 -> b67[]
    Cons3 -> b74[t0]

