data Maybe (a :: *)
  = Nothing
  | Just a

-----------------------------------------
-- not recursive
Nothing :: forall (a :: *). Maybe a
Nothing <-
  Nothing()

-----------------------------------------
-- not recursive
entrypoint bar :: Maybe Bool
bar <-
  return Nothing

-----------------------------------------
-- not recursive
k51 :: forall (a :: *). {} [a] ->> [Maybe a]
k51{} t0 = Just(t0)

-----------------------------------------
-- not recursive
b76 :: forall (a :: *). [] >>= [[a] ->> [Maybe a]]
b76[] =
  t0 <- k51{}
  return t0

-----------------------------------------
-- not recursive
Just :: forall (a :: *). [a] ->> [Maybe a]
Just <-
  b76[]

-----------------------------------------
-- not recursive
b77 :: [] >>= [Maybe Word]
b77[] =
  t0 <- return Just
  t0 @ 3

-----------------------------------------
-- not recursive
entrypoint foo :: Maybe Word
foo <-
  b77[]

