data Maybe (a :: *)
  = Nothing
  | Just a

-----------------------------------------
-- not recursive
Nothing :: forall (a :: *). Maybe a
Nothing <-
  Nothing()

-----------------------------------------
-- not recursive
entrypoint bar :: Maybe Bool
bar <-
  return Nothing

-----------------------------------------
-- not recursive
k43 :: forall (a :: *). {} [a] ->> [Maybe a]
k43{} t0 = Just(t0)

-----------------------------------------
-- not recursive
b64 :: forall (a :: *). [] >>= [[a] ->> [Maybe a]]
b64[] =
  t0 <- k43{}
  return t0

-----------------------------------------
-- not recursive
Just :: forall (a :: *). [a] ->> [Maybe a]
Just <-
  b64[]

-----------------------------------------
-- not recursive
b65 :: [] >>= [Maybe Word]
b65[] =
  t0 <- return Just
  t0 @ 3

-----------------------------------------
-- not recursive
entrypoint foo :: Maybe Word
foo <-
  b65[]

