@fib15 = global i32 0

@fib12 = global i32 0

define i32 @fib(i32 %r0) {
  br label %fib

fib:
  %r1 = icmp eq i32 %r0, 0
  br i1 %r1, label %b82, label %b98

b82:
  ret i32 0

b98:
  %r2 = icmp eq i32 %r0, 1
  br i1 %r2, label %b80, label %b79

b80:
  ret i32 1

b79:
  %r7 = add i32 %r0, -1
  %r4 = call i32 @fib(i32 %r7)
  %r6 = add i32 %r0, -2
  %r5 = call i32 @fib(i32 %r6)
  %r3 = add i32 %r4, %r5
  ret i32 %r3
}

define void @initialize() {
  %r0 = call i32 @fib(i32 15)
  store i32 %r0, i32* @fib15
  %r1 = call i32 @fib(i32 12)
  store i32 %r1, i32* @fib12
  br label %b103

b103:
  ret void
}

