-- not recursive
b69 :: [Word] >>= [Word]
b69[t0] =
  return t0

-----------------------------------------
-- recursive
b70 :: [Word, Word] >>= [Word]
b70[t0, t1] =
  t2 <- add((t0, t1))
  t3 <- add((t1, -1))
  t4 <- primEq((t3, 0))
  if t4
    then b69[t2]
    else b70[t2, t3]

