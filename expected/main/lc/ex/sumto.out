-- not recursive
b89 :: [Word] >>= [Word]
b89[t0] =
  return t0

-----------------------------------------
-- recursive
b88 :: [Word, Word] >>= [Word]
b88[t0, t1] =
  t2 <- add((t0, t1))
  t3 <- add((t1, -1))
  t4 <- primEq((t3, 0))
  if t4
    then b89[t2]
    else b88[t2, t3]

