-- not recursive
b109 :: [] >>= [Word]
b109[] =
  return 0

-----------------------------------------
-- not recursive
entrypoint main :: [Word] >>= [Word]
main[t0] =
  t1 <- primEq((t0, 0))
  if t1
    then b109[]
    else b88[0, t0]

