-- not recursive
b101 :: [Word] >>= [Word]
b101[t0] =
  return t0

-----------------------------------------
-- recursive
b100 :: [Word, Word] >>= [Word]
b100[t0, t1] =
  t2 <- add((t0, t1))
  t3 <- add((t1, -1))
  t4 <- primEq((t3, 0))
  if t4
    then b101[t2]
    else b100[t2, t3]

-----------------------------------------
-- not recursive
entrypoint main :: [Word] >>= [Word]
main[t0] =
  t1 <- primEq((t0, 0))
  if t1
    then b101[0]
    else b100[0, t0]

