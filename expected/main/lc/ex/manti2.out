-- recursive
b105 :: [Word, Word] >>= [Unit]
b105[t0, t1] =
  t2 <- add((t1, -1))
  t3 <- add((t0, t1))
  t4 <- primEq((t2, 0))
  if t4
    then b106[t3]
    else b105[t3, t2]

