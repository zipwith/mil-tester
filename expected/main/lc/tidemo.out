bitdata Bool /1
  = False [ B0 ]
    -- predFalse(x :: Bit 1) = x == 0x0
  | True [ B1 ]
    -- predTrue(x :: Bit 1) = x == 0x1

-- bit pattern:
--  _

bitdata T /8
  = MkT [ x :: Bit 8 ]
    -- predMkT(x :: Bit 8) = true

-- bit pattern:
--  ________

-----------------------------------------
-- not recursive
external f :: [[T] ->> [Bit 8]] ->> [Bool]

-----------------------------------------
-- not recursive
b88 :: [T] >>= [Bit 8]
b88[t0] =
  t1 <- MkT 0 t0
  T.MkT 0 t1

-----------------------------------------
-- not recursive
k55 :: {} [T] ->> [Bit 8]
k55{} t0 = b88[t0]

-----------------------------------------
-- not recursive
s1 :: [T] ->> [Bit 8]
s1 <-
  k55{}

-----------------------------------------
-- not recursive
entrypoint main :: Bool
main <-
  f @ s1

-----------------------------------------
-- Entrypoints: main
