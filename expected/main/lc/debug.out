bitdata Bool /1
  = False [ B0 ]
    -- predFalse(x :: Bit 1) = x == 0x0
  | True [ B1 ]
    -- predTrue(x :: Bit 1) = x == 0x1

-- bit pattern:
--  _

-----------------------------------------
-- not recursive
external prop :: [Bool] ->> [[] ->> [Bool]]

-----------------------------------------
-- not recursive
external resume :: [Bool] ->> [[] ->> [Bit 32]]

-----------------------------------------
-- not recursive
b46 :: [Bool] >>= [Bit 32]
b46[t0] =
  t1 <- resume @ t0
  t1 @ []

-----------------------------------------
-- not recursive
b45 :: [Bool, Bool] >>= [Bit 32]
b45[t0, t1] =
  t2 <- prop @ t0
  t3 <- t2 @ []
  case t3 of
    True -> b46[t1]
    False -> b46[t0]

-----------------------------------------
-- not recursive
k31 :: {Bool, Bool} [] ->> [Bit 32]
k31{t0, t1} [] = b45[t0, t1]

-----------------------------------------
-- not recursive
k27 :: {} [Bool] ->> [[] ->> [Bit 32]]
k27{} t0 = k31{t0, t0}

-----------------------------------------
-- not recursive
entrypoint input :: [Bool] ->> [[] ->> [Bit 32]]
input <-
  k27{}

