.lc: primStructSelect-pos-32  ./milc -pcosoboro -m --32

type SW = Stored Word
type SB = Stored (Bit 8)
struct S [ x :: SW | y :: SB | z :: SB ]
struct T [ u, v, w :: SW ]
struct U [ a :: T | b :: S ]

entrypoint selectX, selectY, selectZ, selectU, selectV, selectW, selectA, selectB
external selectX {primStructSelect S "x" SW} :: Ref S -> Unit -> Ref SW
external selectY {primStructSelect S "y" SB} :: Ref S -> Unit -> Ref SB
external selectZ {primStructSelect S "z" SB} :: Ref S -> Unit -> Ref SB
external selectU {primStructSelect T "u" SW} :: Ref T -> Unit -> Ref SW
external selectV {primStructSelect T "v" SW} :: Ref T -> Unit -> Ref SW
external selectW {primStructSelect T "w" SW} :: Ref T -> Unit -> Ref SW
external selectA {primStructSelect U "a" T}  :: Ref U -> Unit -> Ref T
external selectB {primStructSelect U "b" S}  :: Ref U -> Unit -> Ref S

