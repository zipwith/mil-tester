.lc: primAt-pos-32  ./milc -pcosoboro -m --32

type SW = Stored Word

entrypoint at0SW, at1SW, at2SW, at10SW, at160SW, at256SW, at1KSW, at4KSW
external at0SW   {@   0 SW} :: Ref (Array   0 SW) -> Ix   0 -> Ref SW
external at1SW   {@   1 SW} :: Ref (Array   1 SW) -> Ix   1 -> Ref SW
external at2SW   {@   2 SW} :: Ref (Array   2 SW) -> Ix   2 -> Ref SW
external at10SW  {@  10 SW} :: Ref (Array  10 SW) -> Ix  10 -> Ref SW
external at160SW {@ 160 SW} :: Ref (Array 160 SW) -> Ix 160 -> Ref SW
external at256SW {@ 256 SW} :: Ref (Array 256 SW) -> Ix 256 -> Ref SW
external at1KSW  {@  1K SW} :: Ref (Array  1K SW) -> Ix  1K -> Ref SW
external at4KSW  {@  4K SW} :: Ref (Array  4K SW) -> Ix  4K -> Ref SW

type SI = Stored (Ix 256)

entrypoint at0SI, at1SI, at2SI, at10SI, at160SI, at256SI, at1KSI, at4KSI
external at0SI   {@   0 SI} :: Ref (Array   0 SI) -> Ix   0 -> Ref SI
external at1SI   {@   1 SI} :: Ref (Array   1 SI) -> Ix   1 -> Ref SI
external at2SI   {@   2 SI} :: Ref (Array   2 SI) -> Ix   2 -> Ref SI
external at10SI  {@  10 SI} :: Ref (Array  10 SI) -> Ix  10 -> Ref SI
external at160SI {@ 160 SI} :: Ref (Array 160 SI) -> Ix 160 -> Ref SI
external at256SI {@ 256 SI} :: Ref (Array 256 SI) -> Ix 256 -> Ref SI
external at1KSI  {@  1K SI} :: Ref (Array  1K SI) -> Ix  1K -> Ref SI
external at4KSI  {@  4K SI} :: Ref (Array  4K SI) -> Ix  4K -> Ref SI

type SS = Stored (Bit 16)

entrypoint at0SS, at1SS, at2SS, at10SS, at160SS, at256SS, at1KSS, at4KSS
external at0SS   {@   0 SS} :: Ref (Array   0 SS) -> Ix   0 -> Ref SS
external at1SS   {@   1 SS} :: Ref (Array   1 SS) -> Ix   1 -> Ref SS
external at2SS   {@   2 SS} :: Ref (Array   2 SS) -> Ix   2 -> Ref SS
external at10SS  {@  10 SS} :: Ref (Array  10 SS) -> Ix  10 -> Ref SS
external at160SS {@ 160 SS} :: Ref (Array 160 SS) -> Ix 160 -> Ref SS
external at256SS {@ 256 SS} :: Ref (Array 256 SS) -> Ix 256 -> Ref SS
external at1KSS  {@  1K SS} :: Ref (Array  1K SS) -> Ix  1K -> Ref SS
external at4KSS  {@  4K SS} :: Ref (Array  4K SS) -> Ix  4K -> Ref SS

type SA = Array 23 (Stored (Bit 16))

entrypoint at0SA, at1SA, at2SA, at10SA, at160SA, at256SA, at1KSA, at4KSA
external at0SA   {@   0 SA} :: Ref (Array   0 SA) -> Ix   0 -> Ref SA
external at1SA   {@   1 SA} :: Ref (Array   1 SA) -> Ix   1 -> Ref SA
external at2SA   {@   2 SA} :: Ref (Array   2 SA) -> Ix   2 -> Ref SA
external at10SA  {@  10 SA} :: Ref (Array  10 SA) -> Ix  10 -> Ref SA
external at160SA {@ 160 SA} :: Ref (Array 160 SA) -> Ix 160 -> Ref SA
external at256SA {@ 256 SA} :: Ref (Array 256 SA) -> Ix 256 -> Ref SA
external at1KSA  {@  1K SA} :: Ref (Array  1K SA) -> Ix  1K -> Ref SA
external at4KSA  {@  4K SA} :: Ref (Array  4K SA) -> Ix  4K -> Ref SA

