type SW = Stored Word
type SB = Stored (Bit 8)
struct S [ x :: SW | y :: SB ]

entrypoint bad0, bad1, bad2, bad3, bad4, bad5
external bad0 {@ 0}       :: Ref (Array 8 SW) -> Ix 8 -> Ref SW
external bad1 {@ Word}    :: Ref (Array 8 SW) -> Ix 8 -> Ref SW
external bad2 {@ 8 SW}    :: Ref (Array 5 SW) -> Ix 8 -> Ref SW -- won't catch :-(
external bad3 {@ 4 S}     :: Ref (Array 4  S) -> Ix 4 -> Ref S
external bad4 {@ 4 Word}  :: Ref (Array 4 SW) -> Ix 4 -> Ref SW
external bad5 {@ 4G Word} :: Ref (Array 4G SW) -> Ix 4G -> Ref SW

